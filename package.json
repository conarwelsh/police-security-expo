{
  "name": "huck.boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "babel": "babel src --out-dir dist",
    "build": "npm run babel && npm run webpack",
    "debug": "DEBUG=cw.* npm start",
    "lint": "eslint src",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "serve": "nodemon dist/server",
    "start": "npm run babel -- -w & npm run serve & webpack -w",
    "test": "mocha --compilers js:babel-register -c -G -R min --check-leaks --recursive 'src/**/*.test.js'",
    "test:json": "npm run test:all -- -R json",
    "webpack": "webpack --progress"
  },
  "author": "Conar Welsh <conarw@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "apollo-client": "^0.4.15",
    "apollo-server": "^0.2.8",
    "body-parser": "^1.15.2",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "graphql": "^0.7.0",
    "graphql-tag": "^0.1.14",
    "graphql-tools": "^0.7.2",
    "knex": "^0.11.10",
    "lodash": "^4.15.0",
    "moment": "^2.15.0",
    "mysql": "^2.11.1",
    "pug": "^2.0.0-beta6",
    "radium": "^0.18.1",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.2",
    "react-apollo": "^0.5.5",
    "react-dom": "^15.3.1",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "remarkable": "^1.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-root-import": "^4.1.1",
    "eslint": "^3.5.0",
    "eslint-plugin-graphql": "^0.2.4",
    "faker": "^3.1.0",
    "mocha": "^3.0.2",
    "webpack": "^1.13.2",
    "webpack-builder": "^1.0.0-0"
  }
}
